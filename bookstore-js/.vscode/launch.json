  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Web API",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Web API",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\server.js"
    }
  ]
}
